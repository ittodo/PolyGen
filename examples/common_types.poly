// 공용 타입을 이 파일에 정의
namespace game.common {
    /// 스킬이나 공격의 속성을 나타내는 열거형입니다.
    enum Element {
        /// 테스트
        PHYSICAL;
        FIRE;
        ICE;
        LIGHTNING;
    }

    /// 게임 월드 내의 2D 좌표를 나타내는 복합 타입입니다.
    /// `embed`를 사용하여 여러 테이블에서 재사용할 수 있습니다.
    embed Position {
        // 테스트2
        x: f32;
        y: f32;
    }

    /// 캐릭터의 기본 능력치를 묶은 구조체입니다.
    embed StatBlock {
        health: u32;
        mana: u32;
        attack: u32;
        defense: u32;
    }
}