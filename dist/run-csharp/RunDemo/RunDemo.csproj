<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\..\..\output\csharp\Common\*.cs" Link="Common\%(Filename)%(Extension)" />
    <Compile Include="..\..\..\output\csharp\*.cs"
             Exclude="..\..\..\output\csharp\CsvSchemas\**\*.*;..\..\..\output\csharp\*.BinaryReaders.cs;..\..\..\output\csharp\*.BinaryWriters.cs;..\..\..\output\csharp\*.csproj"
             Link="Generated\%(Filename)%(Extension)" />
    <Compile Include="Attributes.cs" />
  </ItemGroup>
</Project>
