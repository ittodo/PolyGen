namespace {{namespace.name}}
{
%for item in namespace.items
%if item.is_struct
%include "section/struct_block" with item.as_struct, indent=1
%elif item.is_enum
%include "section/enum_block" with item.as_enum, indent=1
%elif item.is_comment
    // {{item.as_comment}}
%elif item.is_namespace
%-- Nested namespace: close current and open nested
%endif
%endfor
}
%blank
%for item in namespace.items
%if item.is_namespace
%include "section/namespace_block" with item.as_namespace
%endif
%endfor
